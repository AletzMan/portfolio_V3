<style>
	.containerLogo {
		position: absolute;
		left: 1em;
		display: flex;
		align-items: center;
		margin: 0 0 0 1em;
		padding: 0 1em 0 0.75em;
		background-color: var(--textColor);
		border: 1px solid #57556acc;
		border-radius: var(--borderRadius);

		@media (width < 768px) {
			left: 0;
		}

		& > svg {
			width: 3.2em;
			height: 3.2em;
			z-index: 1;
		}

		& .containerLogo_div {
			position: absolute;
			left: 0;
			height: 100%;
			width: 30%;
			border-radius: var(--borderRadius) 0 0 var(--borderRadius);
			background-color: var(--textColor);
			border-right: 3px solid var(--backgroundColor);

			@media (width < 768px) {
				width: 50%;
			}
		}

		& .containerLogo_name {
			font-size: 0.5em;
			line-height: 1.2em;
			font-weight: bold;
			color: var(--backgroundColor);
			/*background-color: red;*/
			font-family: Tahoma, Verdana, Geneva, sans-serif;

			@media (width < 768px) {
				display: none;
			}
		}
	}
</style>
