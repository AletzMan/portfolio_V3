---
interface Props {
	class?: string
}
const { class: className, ...rest } = Astro.props
---

<svg class={`iconArrowLeft ${className}`} {...rest} width="1em" height="1em" viewBox="0 0 24 24">
	<path
		fill="none"
		stroke="currentColor"
		stroke-linecap="round"
		stroke-linejoin="round"
		d="m14 7l-5 5l5 5"></path>
</svg>
<style>
	.iconArrowLeft {
		transform: scale(1.4);
	}
</style>
